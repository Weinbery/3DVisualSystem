<body class="login-img3-body">
<div class="container">
  <form class="login-form" method="post" enctype="multipart/form-data">
    <div class="login-wrap">
      <div class="input-group">
        <span class="input-group-addon">用 户 名</span>
        <input type="text" class="form-control" placeholder="用户名" name="username" autofocus>
      </div>
      <div class="input-group">
        <span class="input-group-addon">密 码</span>
        <input type="password" class="form-control" placeholder="密码" name="password">
      </div>
      <div class="input-group">
        <span class="input-group-addon">重复密码</span>
        <input type="password" class="form-control" placeholder="重复密码" name="repassword">
      </div>
      <div class="input-group">
        <span class="input-group">角 色</span>
        <select class="input-group-addon" name="gender" style="width: 308px; height: 40px;">
          <option value="m">管理员</option>
          <option value="f">一般用户</option>
          <option value="x">游客</option>
        </select>
      </div>
      <div class="input-group">
        <span class="input-group">头 像</span>
        <input style="width: 308px; height: 40px;" type="file" name="avatar">
      </div>
      <div class="input-group">
        <span class="input-group">个人简介</span>
        <textarea class="input-group-addon" name="bio" rows="5" style="width: 308px; text-align: left;"></textarea>
      </div>
      <input type="submit" class="btn btn-info btn-lg btn-block" value="注册">
      <!--<button class="btn btn-info btn-lg btn-block" type="submit">注册</button>-->
    </div>
  </form>

</div>

<!--include copyright % -->

</body>
